---
import Image from '@components/ui/images/Image.astro'
import logo from '@images/icon.png'
---

<section class="mx-auto max-w-[85rem] px-4 py-10 sm:px-6 lg:px-8 lg:py-14 2xl:max-w-full">
    <div class="flex flex-col items-center justify-center gap-y-2 sm:flex-row sm:gap-x-12 sm:gap-y-0 lg:gap-x-24">
        <div class="max-w-5xl px-4 sm:px-6 lg:px-8 mx-auto">
            <div class="grid sm:grid-cols-2 lg:grid-cols-1 gap-3 sm:gap-6">

                <div class="p-4 md:p-5">
                    </div>
                    <div class="p-4 md:p-5">
                    </div>
                    
                <div class="col-span-full flex justify-center sm:grid sm:col-span-1">
                    <img src="/public/icon.png" alt="Logo" width="150"/>
                </div>
                <div class="p-4 md:p-5">
                </div>
                <div class="p-4 md:p-5">
                </div>
                <div class="p-4 md:p-5">
                </div>
                <div class="p-4 md:p-5">
                </div>
                <a class="group flex flex-col bg-neutral-100 dark:bg-neutral-900/30 rounded-xl hover:group" href="/home">
                    <div class="p-4 md:p-5">
                        <div class="flex">
                            <div class="grow">
                                <span class="block text-lg text-center font-bold text-neutral-600 group-hover:text-orange-400 dark:text-neutral-300 dark:group-hover:text-orange-300">Enter</span>
                            </div>
                        </div>
                    </div>
                </a>

            </div>
        </div>
    </div>
</section>
